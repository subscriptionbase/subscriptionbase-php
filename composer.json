{
  "name": "subscriptionbase/subscriptionbase-php",
  "description": "SubscriptionBase PHP Library",
  "keywords": [
    "subscriptionbase"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "SubscriptionBase and contributors"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "~0.6.1",
    "squizlabs/php_codesniffer": "~2.0",
  },
  "autoload": {
    "psr-4": { "SubscriptionBase\\" : "lib/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.*-dev"
    }
  }
}
